package nasir;

public class Tes  extends Thread {
	public void run() {
		System.out.println("child thread");
		System.out.println(Thread.currentThread().getPriority());
	}
	public static void main(String[]args) {
		System.out.println(Thread.currentThread().getPriority());
		Tes t=new Tes();
		t.start();
		
		
		
	}}