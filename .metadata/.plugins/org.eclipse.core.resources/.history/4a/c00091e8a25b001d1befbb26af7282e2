package nasirk;


class A{
	void m1() {
		System.out.println("im nasir");
	}
	
}
class Demo extends A{
	void m1() {
		System.out.println("i m ibraheem");
	}
	public static void main(String[] args) {
		A a=new Demo();
		a.m1();
	}

}




